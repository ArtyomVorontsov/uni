module display_3_decoder (
  input x1,
  input x2,
  input x3,
  output M,
  output TL,
  output T,
  output TR,
  output BL,
  output B,
  output BR,
  output D
);

  assign M = (x1 & x2 & x3);
  assign TL = (x1 & x2 & x3);
  assign T = (x1 & x2 & x3);
  assign TR = 0;
  assign BL = (x1 & x2 & x3);
  assign B = (x1 & x2 & x3);
  assign BR = (x1 & x2 & x3);
  assign D = 0;

endmodule


